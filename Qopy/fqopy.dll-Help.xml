<?xml version="1.0" encoding="utf-8"?>
<helpItems xmlns="http://msh" schema="maml">
	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<command:details>
			<command:name>Copy-Files</command:name>
			<maml:description>
				<maml:para>A Powershell File Copy Module with CRC Check.</maml:para>
			</maml:description>
			<command:verb>Copy</command:verb>
			<command:noun>Files</command:noun>
		</command:details>
		<maml:description>
			<maml:para>Fqopy is a binary Powershell Module that provides some the functionality of XCopy, Robocopy and Teracopy with an object output for advanced reporting and auditing.</maml:para>
		</maml:description>
		<command:syntax>
			<command:syntaxItem>
				<maml:name>Copy-Files</maml:name>
				<command:parameter required="true" position="1">
					<maml:name>Source</maml:name>
					<command:parameterValue required="true">String</command:parameterValue>
				</command:parameter>
				<command:parameter required="true" position="2">
					<maml:name>Destination</maml:name>
					<command:parameterValue required="true">String</command:parameterValue>
				</command:parameter>
				<command:parameter required="false" position="3">
					<maml:name>Filter</maml:name>
					<command:parameterValue required="true">String</command:parameterValue>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>Recurse</maml:name>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>Overwrite</maml:name>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>Fast</maml:name>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>ShowProgress</maml:name>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>List</maml:name>
				</command:parameter>
				<command:parameter required="false">
					<maml:name>PassThru</maml:name>
				</command:parameter>
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			<command:parameter required="true" position="1">
				<maml:name>Source</maml:name>
				<maml:description>
					<maml:para>Source Directory</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="true" position="2">
				<maml:name>Destination</maml:name>
				<maml:description>
					<maml:para>Destination Directory</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false" position="3">
				<maml:name>Filter</maml:name>
				<maml:description>
					<maml:para>Filename string filter. Accepts * and ? wildcard characters.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>Recurse</maml:name>
				<maml:description>
					<maml:para>Copy files in all sub-directories of Source.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>Overwrite</maml:name>
				<maml:description>
					<maml:para>Overwrite files in the Destination if they exist.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>Fast</maml:name>
				<maml:description>
					<maml:para>Don't copy the Create, Access and LastWrite TimeStamps.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>ShowProgress</maml:name>
				<maml:description>
					<maml:para>Show a progress bar with time estimate.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>List</maml:name>
				<maml:description>
					<maml:para>Specifies the path to a text file which contains list of relative paths of files to copy.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required="false">
				<maml:name>PassThru</maml:name>
				<maml:description>
					<maml:para>Passes the copying result objects to the pipeline. By default, this cmdlet does not generate any output. Needed for Get-CopyResultsReport cmdlet.</maml:para>
				</maml:description>
			</command:parameter>
		</command:parameters>
		<command:inputTypes>
			<command:inputType>
				<dev:type/>
			</command:inputType>
		</command:inputTypes>
		<command:returnValues>
			<command:returnValue>
				<dev:type>
					<maml:name>Fqopy.FileCopyResultsItem</maml:name>
				</dev:type>
				<maml:description>
					<maml:para>One result item for each Source file with Size, CRC and Match properties.</maml:para>
				</maml:description>
			</command:returnValue>
		</command:returnValues>
		<maml:alertSet>
			<maml:alert>
				<maml:para>Using -ShowProgress without capturing the Cmdlet output can cause a very flashy screen.</maml:para>
			</maml:alert>
		</maml:alertSet>
		<command:examples>
			<command:example>
				<maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
				<dev:code>PS C:\&gt; Copy-Files C:\users\downloads D:\filearchive -Recurse</dev:code>
				<dev:remarks>
					<maml:para>Copy all file and files in all sub-directories of &quot;C:\users\downloads&quot; to &quot;D:\filearchive&quot;.</maml:para>
				</dev:remarks>
			</command:example>
			<command:example>
				<maml:title>-------------------------- EXAMPLE 2 --------------------------</maml:title>
				<dev:code>PS C:\&gt; Copy-Files C:\users\downloads D:\filearchive -Recurse -List C:\users\downloads\queue.txt</dev:code>
				<dev:remarks>
					<maml:para>Copy files of &quot;C:\users\downloads&quot; to &quot;D:\filearchive&quot; which specified in &quot;C:\users\downloads\queue.txt&quot;</maml:para>
				</dev:remarks>
			</command:example>
		</command:examples>
		<maml:relatedLinks>
			<maml:navigationLink>
				<maml:linkText>Fqopy on Github</maml:linkText>
				<maml:uri>https://github.com/rbobot/fqopy</maml:uri>
			</maml:navigationLink>
		</maml:relatedLinks>
	</command:command>
	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<command:details>
			<command:name>Get-CopyResultsReport</command:name>
			<maml:description>
				<maml:para>Aggregate output of Copy-Files</maml:para>
			</maml:description>
			<command:verb>Get</command:verb>
			<command:noun>CopyResultsReport</command:noun>
		</command:details>
		<maml:description>
			<maml:para>Aggregate output of Fqopy module command Copy-Files.</maml:para>
		</maml:description>
		<command:syntax>
			<command:syntaxItem>
				<maml:name>Get-CopyResultsReport</maml:name>
				<command:parameter required="true" position="named">
					<maml:name>InputObject</maml:name>
					<command:parameterValue required="true">FileCopyResultsItem</command:parameterValue>
				</command:parameter>
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			<command:parameter required="true" pipelineInput="true (ByValue)" position="named">
				<maml:name>InputObject</maml:name>
				<maml:description>
					<maml:para>Output of Copy-Files</maml:para>
				</maml:description>
			</command:parameter>
		</command:parameters>
		<command:inputTypes>
			<command:inputType>
				<dev:type>
					<maml:name>Fqopy.FileCopyResultItem</maml:name>
				</dev:type>
				<maml:description>
					<maml:para>Output of Copy-Files</maml:para>
				</maml:description>
			</command:inputType>
		</command:inputTypes>
		<command:returnValues>
			<command:returnValue>
				<dev:type>
					<maml:name>Fqopy.FileCopyResultsReport</maml:name>
				</dev:type>
				<maml:description>
					<maml:para>Aggregate stats of Copy-Files output</maml:para>
				</maml:description>
			</command:returnValue>
		</command:returnValues>
		<command:examples>
			<command:example>
				<maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
				<dev:code>PS C:\&gt; $report = Copy-Files C:\users\downloads D:\filearchive -Recurse -PassThru | Get-CopyResultsReport</dev:code>
				<dev:remarks>
					<maml:para>Aggregates the output of Copy-Files into the &quot;report&quot; variable.</maml:para>
				</dev:remarks>
			</command:example>
		</command:examples>
		<maml:relatedLinks>
			<maml:navigationLink/>
		</maml:relatedLinks>
	</command:command>
</helpItems>